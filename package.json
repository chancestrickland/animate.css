{
  "name": "@chancedigital/animate.scss",
  "version": "3.7.0",
  "main": "animate.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/chancestrickland/animate.scss"
  },
  "author": {
    "name": "Chance Strickland"
  },
  "homepage": "https://daneden.github.io/animate.css/",
  "license": "MIT",
  "jspm": {
    "main": "animate.scss",
    "format": "global",
    "directories": {
      "lib": "./"
    }
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "cssnano": "^4.0.3",
    "eslint": "^6.2.2",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "postcss-import": "^12.0.1",
    "prettier": "^1.10.2",
    "run-sequence": "^2.2.1"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write",
      "git add"
